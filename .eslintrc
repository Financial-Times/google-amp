{
	"extends": "airbnb/base",
	"env": {
		"node": true
	},
	"parserOptions": {
		"sourceType": "script"
	},
	"rules": {
		"indent": [2, "tab"],
		"comma-dangle": [2, "always-multiline"],
		"keyword-spacing": [2, {
			"before": true,
			"after": false,
			"overrides": {
				"else": {"after": true},
				"return": {"after": true},
				"case": {"after": true}
			}
		}],
		"max-len": [1, 120, 2],
		"object-curly-spacing": [2, "never"],
		"space-before-function-paren": [2, "never"],
		"strict": [2, "global"],
		"no-else-return": 0,
		"no-param-reassign": 0,
		"no-console": 0,
		"consistent-return": 0,
		"object-shorthand": 1,
		"prefer-rest-params": 0
	}
}
